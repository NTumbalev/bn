<nav class="breadcrumbs">
	<ul>
		<li><a href="{{path('homepage')}}">{{ 'homepage'|trans({}, "messages")}}</a></li>
		{% for key, item in breadCrumbs %}
		    <li>{% if not loop.last %}<a href="{{item}}">{% endif %}{{key}}{% if item is not null and not loop.last %}</a>{% endif %}</li>
		{% endfor %}
	</ul>
</nav>
